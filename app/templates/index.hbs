
<div class="row">
  <div class="col-sm-6">
    <h3 class="text-center">Questions</h3>
    {{#each model.questions as |question|}}
      {{question-tile question=question}}
    {{/each}}
  </div>
  <div class="col-sm-6 text-center">
    {{new-question saveQuestion="saveQuestion"}}
  </div>
</div>
